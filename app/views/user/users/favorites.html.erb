<div class="container">
  <div class="col-md-12">
     <h2 class="mt-5">いいね一覧</h2>
        <table class="table table-hover table-inverse">
          <thead>
              <tr>
                <th class="col-3">タイトル</th>
                <th>内容</th>
              </tr>
          </thead>
          <tbody>
            <% @favorite_posts.each do |post|%>
            <tr>
              <td><%= link_to post.title, user_post_path(post.id) %></td>
              <td class="line-break"><%= post.body %></td>
            </tr>
          </tbody>
          <% end %>
        </table>
        
        
        
  </div>
</div>